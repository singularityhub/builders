---
title:  "Ubuntu on Google Compute Engine"
date:   2018-03-16 12:00:00
layout: bundle
author: Vanessa Sochat
runscript: run.sh
tags: 
 - ubuntu
 - singularity
metadata:
 - key: GOOGLE_COMPUTE_PROJECT
   value: "debian-cloud"
 - key: SREGISTRY_BUILDER_machine_type
   value: "n1-standard-1"
 - key: GOOGLE_COMPUTE_IMAGE_FAMILY
   value: "debian-8"
 - key: SINGULARITY_BRANCH
   value: "feature-squashbuild-secbuild-2.4.3"
 - key: SINGULARITY_REPO
   value: "https://github.com/cclerget/singularity.git"
 - key: SREGISTRY_BUILDER_RUNSCRIPT
   value: "run.sh"
 - key: SREGISTRY_BUILDER_KILLHOURS
   value: "10"
 - key: SREGISTRY_GOOGLE_STORAGE_PRIVATE
   value: false
 - key: SREGISTRY_BUILDER_BRANCH
 - key: SREGISTRY_BUILDER_TAG
 - key: SREGISTRY_BUILDER_COMMIT
 - key: SINGULARITY_RECIPE
 - key: SINGULARITY_COMMIT
 - key: SREGISTRY_CONTACT
---

        {
            "disks": [],
            "labels": [{
                "key": "sregistry",
                "value": "builder"
             }],
            "tags": {
               "items": [
                    "http-server",
                    "https-server"
            ]},
            "networkInterfaces": [
                {
                    "accessConfigs": [
                        {
                            "name": "External NAT",
                            "type": "ONE_TO_ONE_NAT"
                        }
                    ],
                    "network": "global/networks/default"
                }
            ],
            "serviceAccounts": [
                {
                    "email": "default",
                    "scopes": [
                        "https://www.googleapis.com/auth/compute",
                        "https://www.googleapis.com/auth/devstorage.read_write",
                        "https://www.googleapis.com/auth/logging.write"
                    ]
                }
            ]
        }
